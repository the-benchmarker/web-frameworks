{
  "name": "antidot-fw/reactive-starter",
  "description": "Reactive Antidot Framework Apllication Starter",
  "type": "project",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "kpicaza"
    }
  ],
  "require": {
    "ext-json": "*",
    "antidot-fw/framework": "~2.0",
    "antidot-fw/cli": "@stable",
    "antidot-fw/container": "@stable",
    "antidot-fw/runtime": "@stable",
    "fidry/cpu-core-counter": "@stable",
    "laminas/laminas-config-aggregator": "@stable"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
